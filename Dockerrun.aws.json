{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "host": {
        "sourcePath": "/efs"
      },
      "name": "Efs"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "dperson/samba:latest",
      "name": "samba",
      "memory": 930,
      "command": [
        "-u",
        "samba;password",
        "-s",
        "HPA;/efs",
        "-n"
      ],
       "mountPoints": [
        {
          "containerPath": "/efs",
          "sourceVolume": "Efs"
        }
      ],
      "portMappings": [
        {
          "containerPort": 137,
          "hostPort": 137
        },
        {
          "containerPort": 138,
          "hostPort": 138
        },
        {
          "containerPort": 139,
          "hostPort": 139
        },
        {
          "containerPort": 445,
          "hostPort": 445
        }
      ],
      "privileged": "true"
    }
  ]
}

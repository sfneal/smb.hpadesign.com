{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "host": {
        "sourcePath": "/efs"
      },
      "name": "Efs"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "stephenneal/samba-efs:v2",
      "name": "samba",
      "memory": 930,
      "environment": {
        "USER1": "stephen;stealth",
        "USER2": "harnaudo;patriots9",
        "USER3": "bob;patriots9",
        "SHARE1": "HPA;/efs;yes;no;no;"
      },
      "mountPoints": [
        {
          "containerPath": "/efs",
          "sourceVolume": "Efs"
        }
      ],
      "portMappings": [
        {
          "containerPort": 137,
          "hostPort": 137
        },
        {
          "containerPort": 138,
          "hostPort": 138
        },
        {
          "containerPort": 139,
          "hostPort": 139
        },
        {
          "containerPort": 445,
          "hostPort": 445
        }
      ],
      "privileged": "true"
    }
  ]
}
